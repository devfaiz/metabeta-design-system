{
  "name": "metabeta-design-system",
  "version": "1.0.40",
  "description": "\"Metabeta Design System\"",
  "main": "dist/metabeta-design-system.ssr.js",
  "browser": "dist/metabeta-design-system.esm.js",
  "module": "dist/metabeta-design-system.esm.js",
  "unpkg": "dist/metabeta-design-system.min.js",
  "inlineDynamicImports": true,
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "compile:sass": "node-sass src/assets/styles/dna.scss src/assets/css/dna.css",
    "serve": "vue-cli-service serve docs/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metabeta-platform/metabeta-design-system.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "scss",
    "design",
    "metabeta",
    "scss"
  ],
  "author": "oS- Metabeta Frontend Team with <3",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metabeta-platform/metabeta-design-system/issues"
  },
  "homepage": "https://github.com/metabeta-platform/metabeta-design-system#readme"
}
